@page "/counter"
@using BlazorProject.Models

<h1>Counter</h1>

<p>Current count: @currentCount</p>

<button class="btn btn-primary" onclick="@IncrementCount">Click me</button>

@functions {

   public EmailDetail em = new EmailDetail();

    int currentCount = 0;

    void IncrementCount()
    {
        
        currentCount++;
    }
}
